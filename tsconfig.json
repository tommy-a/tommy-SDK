{
    "compilerOptions": {
        "outDir": "./dist/",
        "module": "commonjs",
        "noImplicitAny": true,
        "target": "es5",
        "lib": [
            "es5",
            "es6",
            "es2017",
            "es2019"
        ],
        "strictNullChecks": true,
        "downlevelIteration": true,
        "sourceMap": true,
        "typeRoots": [
            "./node_modules/@types",
        ]
    }
}